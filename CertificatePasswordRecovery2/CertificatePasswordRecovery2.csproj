<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <StartupObject>CertificatePasswordRecovery2.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="CertPasswordRecoveryForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="CertPasswordRecoveryForm.Designer.cs">
      <DependentUpon>CertPasswordRecoveryForm.cs</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="CertPasswordRecoveryForm.resx">
      <DependentUpon>CertPasswordRecoveryForm.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

</Project>